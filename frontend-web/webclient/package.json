{
  "name": "sducloud",
  "version": "0.20.0",
  "description": "SDUCloud",
  "author": "IMADA - eScience Center, SDU",
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "html",
      "cobertura"
    ],
    "modulePaths": [
      "app"
    ],
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "moduleNameMapper": {
      "^.+\\.(png|ttf|jpg)$": "file-loader",
      "^.+\\.css$": "css-loader",
      "Authentication/SDUCloudObject": "<rootDir>/__tests__/mock/Cloud.ts"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)\\.(test|spec))\\.(j|t)sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "ts-jest",
    "testMatch": null,
    "globals": {
      "ts-jest": {
        "ts-config": "tsconfig.json",
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    }
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --inline --progress --port 9000",
    "clean": "rm -rf dist/",
    "build": "npm run clean && webpack --config webpack.prod.js --progress --profile --bail",
    "test": "jest",
    "prepare-icons": "npm run icons-clean && npm run icons-components && npm run icons-index",
    "icons-clean": "cd app/ui-components/icons; rm -rf components index.ts",
    "icons-components": "cd app/ui-components/icons; svgr . --ext=tsx --filename-case camel -d components",
    "icons-index": "node scripts/create-icons-index.js"
  },
  "dependencies": {
    "@novnc/novnc": "^1.1.0",
    "@svgr/cli": "^4.3.2",
    "fuse.js": "^3.4.5",
    "jsonwebtoken": "8.5.1",
    "moment": "~2.24.0",
    "moment-timezone": "^0.5.26",
    "react": "16.9.0",
    "react-datepicker": "2.8.0",
    "react-dom": "16.9.0",
    "react-dropzone": "^10.1.7",
    "react-markdown": "^4.1.0",
    "react-modal": "^3.10.1",
    "react-redux": "^7.1.1",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "recharts": "^1.7.1",
    "redux": "4.0.4",
    "redux-responsive": "^4.3.8",
    "styled-components": "^4.3.2",
    "styled-system": "^5.1.1",
    "svgi": "^1.1.0",
    "xterm": "^3.14.5"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/history": "^4.7.3",
    "@types/jest": "24.0.18",
    "@types/json-schema": "^7.0.3",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/moment-timezone": "^0.5.12",
    "@types/react": "16.9.2",
    "@types/react-datepicker": "2.8.1",
    "@types/react-dom": "^16.9.0",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-modal": "3.8.2",
    "@types/react-redux": "^7.1.2",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-test-renderer": "16.9.0",
    "@types/recharts": "^1.1.21",
    "@types/styled-components": "4.1.18",
    "@types/styled-system": "^5.1.1",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "5.0.4",
    "css-loader": "^3.2.0",
    "empty-module": "~0.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "0.8.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.3",
    "mini-css-extract-plugin": "^0.8.0",
    "react-test-renderer": "16.9.0",
    "redux-devtools-extension": "^2.13.8",
    "source-map-loader": "^0.2.4",
    "style-loader": "1.0.0",
    "terser-webpack-plugin": "^1.4.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "6.0.4",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3",
    "url-loader": "2.1.0",
    "webpack": "4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "3.8.0",
    "webpack-merge": "4.2.1"
  }
}
